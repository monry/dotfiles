##############
# エイリアス #
##############

alias ll='exa -laBgh --icons --git --time-style long-iso'
alias vi='vim'
alias cat='bat'

# Docker
alias dcbu="docker-compose build"
alias dcst="docker-compose stop"
alias dcup="docker-compose up -d"
alias dcdw="docker-compose down"
alias dcex="docker-compose exec"
alias dcps="docker-compose ps"
alias dclog="docker-compose logs"
alias dcru="docker-compose run"

# NuGet
alias nuget="mono /usr/local/bin/nuget.exe"

# Chrome
alias chrome='open -a "Google Chrome" $1'

# Change Directory
alias booth='cd ~/Developer/git/kidsstar/booth'
alias sandbox='cd ~/Developer/git/sandbox'
alias pretendland='cd ~/Developer/git/pretendland/pretendland'
alias cnexco='cd ~/Developer/git/pretendland/cnexco'
alias slime='cd ~/Developer/git/kidsstar/PlayWithSlime'
alias zenn='cd ~/Developer/git/monry/Zenn'

