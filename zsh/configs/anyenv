##########
# anyenv #
##########
eval "$(anyenv init -)"

if type "pyenv" > /dev/null 2>&1; then
    eval "$(pyenv init -)"
fi

if type "rbenv" > /dev/null 2>&1; then
    eval "$(rbenv init -)"
fi

if type "nodeenv" > /dev/null 2>&1; then
    eval "$(nodenv init -)"
fi

if type "goenv" > /dev/null 2>&1; then
    eval "$(goenv init -)"
fi

# vim:set ft=zsh:
